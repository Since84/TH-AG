{
    "repositories": [
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "wordpress/core",
                "type": "webroot",
                "version": "4.0",
                "dist": {
                    "url": "http://wordpress.org/wordpress-4.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery-ui/stable",
                "version": "1.11.0",
                "dist": {
                    "url": "http://jqueryui.com/resources/download/jquery-ui-1.11.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "fancyguy/webroot-installer": "1.0.0",
        "wordpress/core": "4.0",
        "wpackagist-plugin/json-api": "1.1.*",
        "wpackagist-plugin/timber-library": "0.20.*",
        "wpackagist-plugin/seo-ultimate": "7.6.5.*",
        "wpackagist-plugin/w3-total-cache": "0.9.*",
        "wpackagist-plugin/advanced-custom-fields": "4.4.*",
        "wpackagist-plugin/social-media-icons-widget": "14.*",
        "wpackagist-plugin/w3-total-cache": "0.9.*",
        "wpackagist-plugin/typekit-fonts-for-wordpress": "1.*",
        "wpackagist-plugin/font-awesome": "3.2.*",
        "wpackagist-plugin/display-latest-tweets": "1.*",
        "wpackagist-plugin/contact-form-7": "4.*",
        "jquery-ui/stable": "1.11.*"
    },
    "extra": {
      "webroot-dir": "wp",
      "webroot-package": "wordpress/core"
    }
}